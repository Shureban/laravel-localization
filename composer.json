{
  "name": "shureban/laravel-localization",
  "description": "Laravel SDK for searching data in DB using requests",
  "keywords": [
    "localization",
    "laravel",
    "middleware",
    "languages",
    "shureban"
  ],
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Shureban",
      "email": "Shureban@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Shureban\\LaravelLocalization\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shureban\\LaravelLocalization\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Shureban\\LaravelLocalization\\LocalizationServiceProvider"
      ]
    }
  }
}
